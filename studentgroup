<xs:complexType>
<xs:sequence>
<xs:element name="academicArea" type="xs:string"/>
<xs:element name="code" type="xs:string"/>
</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="studentMinors" maxOccurs="unbounded">
<xs:complexType>
<xs:sequence>
<xs:element name="academicArea" type="xs:string"/>
<xs:element name="code" type="xs:string"/>
</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="studentGroups" maxOccurs="unbounded">
<xs:complexType>
<xs:sequence>
<xs:element name="group" type="xs:string"/>
</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="studentAccomodations" maxOccurs="unbounded">
<xs:complexType>
<xs:sequence>
<xs:element name="accomodation" type="xs:string"/>
</xs:sequence>
</xs:complexType>
</xs:element>
</xs:sequence>
</xs:complexType>
</xs:element>
</xs:sequence>
</xs:complexType>
